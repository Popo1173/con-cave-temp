{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxi typecheck",
    "lint:script": "eslint --ext \"src/**/*.js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"src/**/*.{css,scss,sass,vue}\" --ignore-path .gitignore",
    "lint": "yarn typecheck && yarn lint:script && yarn lint:style",
    "fix": "yarn prettier && yarn lint:script --fix && yarn lint:style --fix"
  },
  "dependencies": {
    "nuxt": "^3.10.1",
    "sanitize.css": "^13.0.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/seo": "^2.0.0-rc.8",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "eslint": "^8.56.0",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  }
}
